<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/subscriberListFragment">
    <fragment
        android:id="@+id/subscriberFragment"
        android:name="br.com.backupautomacao.mysubscribers.ui.subscriber.SubscriberFragment"
        android:label="@string/subscriber_title"
        tools:layout="@layout/subscriber_fragment"
        />
    <fragment
        android:id="@+id/subscriberListFragment"
        android:name="br.com.backupautomacao.mysubscribers.MainActivity"
        android:label="subscriber_list_fragment"
        tools:layout="@layout/subscriber_list_fragment" >
        <action
            android:id="@+id/action_main_graph_to_subscriberFragment"
            app:destination="@id/subscriberFragment" />
        <argument
            android:name="subscriber"
            android:defaultValue="@null"
            app:argType="br.com.backupautomacao.mysubscribers.data.db.entity.SubscriberEntity"
            app:nullable="true" />
    </fragment>

</navigation>